[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (isConnect)[m
[32m+[m[32m            {[m
[32m+[m[32m                MongoController.Init();[m
[32m+[m[32m                this.Close();[m
[32m+[m[32m                //TODO: database name yanlÄ±ÅŸ ise veya client baÄŸlantÄ±sÄ± baÅŸarÄ±sÄ±z ise HelpBox mesajÄ± gÃ¶zÃ¼ksÃ¼n -> ramo:tm.[m
[32m+[m[32m                client = client.ConnectionAccount(connectionUrl);[m
[32m+[m[32m                db = client.ConnectionDatabase(databaseName);[m
[32m+[m
[32m+[m[32m                if (db != null)[m
