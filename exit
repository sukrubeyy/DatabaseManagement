[1mdiff --git a/Assets/Scripts/EditorGUISplitView.cs b/Assets/Scripts/EditorGUISplitView.cs[m
[1mnew file mode 100644[m
[1mindex 0000000..afd880b[m
[1m--- /dev/null[m
[1m+++ b/Assets/Scripts/EditorGUISplitView.cs[m
[36m@@ -0,0 +1,95 @@[m
[32m+[m[32musing UnityEngine;[m
[32m+[m[32musing System.Collections;[m
[32m+[m[32musing UnityEditor;[m
[32m+[m
[32m+[m[32mnamespace SplitWindow[m
[32m+[m[32m{[m
[32m+[m[32m    public class EditorGUISplitView[m
[32m+[m[32m    {[m
[32m+[m[32m        public enum Direction[m
[32m+[m[32m        {[m
[32m+[m[32m            Horizontal,[m
[32m+[m[32m            Vertical[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        Direction splitDirection;[m
[32m+[m[32m        float splitNormalizedPosition;[m
[32m+[m[32m        bool resize;[m
[32m+[m[32m        public Vector2 scrollPosition;[m
[32m+[m[32m        Rect availableRect;[m
[32m+[m
[32m+[m
[32m+[m[32m        public EditorGUISplitView(Direction splitDirection)[m
[32m+[m[32m        {[m
[32m+[m[32m            splitNormalizedPosition = 0.5f;[m
[32m+[m[32m            this.splitDirection = splitDirection;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public void BeginSplitView()[m
[32m+[m[32m        {[m
[32m+[m[32m            Rect tempRect;[m
[32m+[m
[32m+[m[32m            if (splitDirection == Direction.Horizontal)[m
[32m+[m[32m                tempRect = EditorGUILayout.BeginHorizontal(GUILayout.ExpandWidth(true));[m
[32m+[m[32m            else[m
[32m+[m[32m                tempRect = EditorGUILayout.BeginVertical(GUILayout.ExpandHeight(true));[m
[32m+[m
[32m+[m[32m            if (tempRect.width > 0.0f)[m
[32m+[m[32m            {[m
[32m+[m[32m                availableRect = tempRect;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (splitDirection == Direction.Horizontal)[m
[32m+[m[32m                scrollPosition = GUILayout.BeginScrollView(scrollPosition, GUILayout.Width(availableRect.width * splitNormalizedPosition));[m
[32m+[m[32m            else[m
[32m+[m[32m                scrollPosition = GUILayout.BeginScrollView(scrollPosition, GUILayout.Height(availableRect.height * splitNormalizedPosition));[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public void Split()[m
[32m+[m[32m        {[m
[32m+[m[32m            GUILayout.EndScrollView();[m
[32m+[m[32m            ResizeSplitFirstView();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public void EndSplitView()[m
[32m+[m[32m        {[m
[32m+[m[32m            if (splitDirection == Direction.Horizontal)[m
[32m+[m[32m                EditorGUILayout.EndHorizontal();[m
[32m+[m[32m            else[m
[32m+[m[32m                EditorGUILayout.EndVertical();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        private void ResizeSplitFirstView()[m
[32m+[m[32m        {[m
[32m+[m[32m            Rect resizeHandleRect;[m
[32m+[m
[32m+[m[32m            if (splitDirection == Direction.Horizontal)[m
[32m+[m[32m                resizeHandleRect = new Rect(availableRect.width * splitNormalizedPosition, availableRect.y, 2f, availableRect.height);[m
[32m+[m[32m            else[m
[32m+[m[32m                resizeHandleRect = new Rect(availableRect.x, availableRect.height * splitNormalizedPosition, availableRect.width, 2f);[m
[32m+[m
[32m+[m[32m            GUI.DrawTexture(resizeHandleRect, EditorGUIUtility.whiteTexture);[m
[32m+[m
[32m+[m[32m            if (splitDirection == Direction.Horizontal)[m
[32m+[m[32m                EditorGUIUtility.AddCursorRect(resizeHandleRect, MouseCursor.ResizeHorizontal);[m
[32m+[m[32m            else[m
[32m+[m[32m                EditorGUIUtility.AddCursorRect(resizeHandleRect, MouseCursor.ResizeVertical);[m
[32m+[m
[32m+[m[32m            if (Event.current.type == EventType.MouseDown && resizeHandleRect.Contains(Event.current.mousePosition))[m
[32m+[m[32m            {[m
[32m+[m[32m                resize = true;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (resize)[m
[32m+[m[32m            {[m
[32m+[m[32m                if (splitDirection == Direction.Horizontal)[m
[32m+[m[32m                    splitNormalizedPosition = Event.current.mousePosition.x / availableRect.width;[m
[32m+[m[32m                else[m
[32m+[m[32m                    splitNormalizedPosition = Event.current.mousePosition.y / availableRect.height;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (Event.current.type == EventType.MouseUp)[m
[32m+[m[32m                resize = false;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
\ No newline at end of file[m
