[32m+[m[32musing MongoDB.Bson;[m
[32m+[m[32musing MongoDB.Driver;[m
[32m+[m[32musing UnityEngine;[m
[32m+[m
[32m+[m[32mnamespace Extentions[m
[32m+[m[32m{[m
[32m+[m[32m    public static class MongoExtentions[m
[32m+[m[32m    {[m
[32m+[m[32m        public static BsonDocument GetCollectionValues(this IMongoDatabase mongo, string collectionName)[m
[32m+[m[32m        {[m
[32m+[m[32m            BsonDocument doc = mongo.GetCollection<BsonDocument>(collectionName).Find(new BsonDocument()).FirstOrDefault();[m
[32m+[m[32m            if (doc is null)[m
[32m+[m[32m                return null;[m
[32m+[m[32m            return doc;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public static void AddCollectionValue(this IMongoDatabase mongo, BsonDocument data, string collectionName)[m
[32m+[m[32m        {[m
[32m+[m[32m            mongo.GetCollection<BsonDocument>(collectionName).InsertOne(data);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public static void UpdateOne(this IMongoDatabase mongo, string columnName, string oldValue, string newValue)[m
[32m+[m[32m        {[m
[32m+[m[32m            //UPDATE[m
[32m+[m[32m            var filter = Builders<BsonDocument>.Filter.Eq(columnName, oldValue);[m
[32m+[m[32m            var filter2 = Builders<BsonDocument>.Update.Set(columnName, newValue);[m
[32m+[m[32m            mongo.GetCollection<BsonDocument>("userInfo").UpdateMany(filter, filter2);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public static void UpdateOne(this IMongoDatabase mongo, string columnName, int oldValue, int newValue)[m
[32m+[m[32m        {[m
[32m+[m[32m            //UPDATE[m
[32m+[m[32m            var filter = Builders<BsonDocument>.Filter.Eq(columnName, oldValue);[m
[32m+[m[32m            var filter2 = Builders<BsonDocument>.Update.Set(columnName, newValue);[m
[32m+[m[32m            mongo.GetCollection<BsonDocument>("userInfo").UpdateMany(filter, filter2);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public static void Delete(this IMongoDatabase mongo, string collectionName, string id, int searchValue)[m
[32m+[m[32m        {[m
[32m+[m[32m            //DELETE[m
[32m+[m[32m            var filter3 = Builders<BsonDocument>.Filter.Eq(id, searchValue);[m
[32m+[m[32m            mongo.GetCollection<BsonDocument>(collectionName).DeleteOne(filter3);[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        public static MongoClient ConnectionAccount(this MongoClient _client, string url)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (!string.IsNullOrEmpty(url))[m
[32m+[m[32m            {[m
[32m+[m[32m                _client = new MongoClient(url);[m
[32m+[m[32m                if (_client != null)[m
[32m+[m[32m                {[m
[32m+[m[32m                    Debug.Log("Connection MongoDB Successfuly");[m
[32m+[m[32m                    return _client;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            return null;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public static IMongoDatabase ConnectionDatabase(this MongoClient _client, string dbName)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (_client != null && !string.IsNullOrEmpty(dbName))[m
[32m+[m[32m            {[m
[32m+[m[32m                return _client.GetDatabase(dbName);[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            return null;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
\ No newline at end of file[m
