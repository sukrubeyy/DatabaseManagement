[32m+[m[32mï»¿using MongoDB.Driver;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing UnityEngine;[m
[32m+[m
[32m+[m[32mpublic class Model_Upload : MonoBehaviour[m
[32m+[m[32m{[m
[32m+[m[32m    private const string MONGO_URI = "mongodb://127.0.0.1:27017";[m
[32m+[m[32m    private const string DATABASE_NAME = "testDatabase";[m
[32m+[m[32m    private MongoClient client;[m
[32m+[m[32m    private IMongoDatabase db;[m
[32m+[m
[32m+[m[32m    void Start()[m
[32m+[m[32m    {[m
[32m+[m[32m        client = new MongoClient(MONGO_URI);[m
[32m+[m[32m        db = client.GetDatabase(DATABASE_NAME);[m
[32m+[m[32m        IMongoCollection<Model_User> userCollection = db.GetCollection<Model_User>("collectionName");[m
[32m+[m[32m        Model_User e = new Model_User();[m
[32m+[m[32m        e.name = "hope";[m
[32m+[m[32m        userCollection.InsertOne(e);[m
[32m+[m[32m        List<Model_User> userModelList = userCollection.Find(user => true).ToList();[m
[32m+[m[32m        Model_User[] userAsap= userModelList.ToArray();[m
[32m+[m[32m        foreach(Model_User asap in userAsap)[m
[32m+[m[32m        {[m
[32m+[m[32m            print(asap.name);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m
[32m+[m[32m}[m
