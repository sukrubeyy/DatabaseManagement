[32m+[m
[32m+[m[32m    private void MongoUI()[m
[32m+[m[32m    {[m
[32m+[m[32m        MongoController mongo = null;[m
[32m+[m[32m        MongoClient client = null;[m
[32m+[m[32m        IMongoDatabase db = null;[m
[32m+[m[32m        GUILayout.BeginVertical();[m
