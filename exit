[32m+[m
[32m+[m[32m             so.ApplyModifiedProperties();[m
[32m+[m
[32m+[m[32m            if (!isConnect)[m
[32m+[m[32m            {[m
[32m+[m[32m                if (GUILayout.Button("Connect"))[m
[32m+[m[32m                {[m[41m   [m
[32m+[m[32m                    if (!string.IsNullOrEmpty(connectionUrl) && !string.IsNullOrEmpty(databaseName))[m[41m [m
