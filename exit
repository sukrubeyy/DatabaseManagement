[32m+[m[32musing System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing MongoDB.Bson;[m
[32m+[m[32musing MongoDB.Driver;[m
[32m+[m[32musing UnityEditor;[m
[32m+[m[32musing SplitWindow;[m
[32m+[m[32mpublic class MongoController : EditorWindow[m
[32m+[m[32m{[m
[32m+[m[41m    [m
[32m+[m[32m    EditorGUISplitView horizontalSplitView = new EditorGUISplitView(EditorGUISplitView.Direction.Horizontal);[m
[32m+[m[32m    EditorGUISplitView verticalSplitView = new EditorGUISplitView(EditorGUISplitView.Direction.Vertical);[m
[32m+[m[41m    [m
[32m+[m[32m    public static void Init()[m
[32m+[m[32m    {[m
[32m+[m[32m        MongoController window = CreateWindow<MongoController>("Mongo Details");[m
[32m+[m[32m        window.Show();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void OnGUI()[m
[32m+[m[32m    {[m
[32m+[m[32m        horizontalSplitView.BeginSplitView ();[m
[32m+[m[32m        horizontalSplitView.Split ();[m
[32m+[m[32m        verticalSplitView.BeginSplitView ();[m
[32m+[m[32m        verticalSplitView.Split ();[m
[32m+[m[32m        verticalSplitView.EndSplitView ();[m
[32m+[m[32m        horizontalSplitView.EndSplitView ();[m
[32m+[m[32m        Repaint();[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
